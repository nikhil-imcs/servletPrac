<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
   <servlet>
  	<servlet-name>GetCustomerServlet</servlet-name>
  	<display-name>GetCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.imcs.servlets.GetCustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveCustomerServlet</servlet-name>
  	<display-name>SaveCustomerServlet</display-name>
  	<description></description>
  	<servlet-class>com.imcs.servlets.SaveCustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetCustomerServlet</servlet-name>
  	<url-pattern>/getCustomer.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveCustomerServlet</servlet-name>
  	<url-pattern>/saveCustomer.do</url-pattern>
  </servlet-mapping>
</web-app>
